<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ikuzo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ikuzo">
        <activity android:name="com.example.ikuzo.SendImage"></activity>
        <activity android:name="com.example.ikuzo.ProfileActivity" />
        <activity android:name="com.example.ikuzo.MessageActivity" />
        <activity android:name="com.example.ikuzo.UnitsActivity" />
        <activity android:name="com.example.ikuzo.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ikuzo.RegisterActivity"
            android:parentActivityName="com.example.ikuzo.StartActivity" />
        <activity android:name="com.example.ikuzo.MainActivity" />
        <activity
            android:name="com.example.ikuzo.LoginActivity"
            android:parentActivityName="com.example.ikuzo.StartActivity" />
    </application>

</manifest>